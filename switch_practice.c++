#include<iostream>
using namespace std;
int main()
{
int day;
cout<<"enter the day num :"<<endl;
cin>>day;
switch(day);
{
	day 1:cout<<"monday";
	break;
	day 2:cout<<"tuesday";
	break;
	day 3:cout<<"wednesday";
	break;
	day 4:cout<<"thursday";
	break;
	day 5:cout<<"friday";
	break;
	day 6:cout<<"saturday";
	break;
	day 7:cout<<"sunday";
	break;
	default:cout<<"invalid";
	break;
}
return 0;
}
